{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://sa-map.onrender.com/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        { 
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}